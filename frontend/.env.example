# Environment Configuration for MyRecipeBox Frontend

# Anonymous Mode - Set to 'true' to enable demo mode with mock data
EXPO_PUBLIC_ANONYMOUS_MODE=false

# API Configuration
# Backend API base URL - update this to match your backend deployment
EXPO_PUBLIC_API_BASE_URL=http://localhost:3000/api

# Development Settings
# Enable debug logging in development
EXPO_PUBLIC_DEBUG=true

# App Configuration
EXPO_PUBLIC_APP_NAME=MyRecipeBox
EXPO_PUBLIC_APP_VERSION=1.0.0